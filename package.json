{
  "name": "ngrx-entity-crud",
  "version": "13.1.0",
  "repository": "https://github.com/jucasoft/ngrx-entity-crud",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ngrx-entity-crud && npm run build:schematics",
    "testLibs": "ng test ngrx-entity-crud",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:schematics": "tsc -p ./projects/ngrx-entity-crud/tsconfig.schematics.json",
    "npm:build:link": "npm run build && cd libs/ngrx-entity-crud/ && npm link",
    "npm:publish": "cd libs/ngrx-entity-crud/ && npm publish",
    "npm:build:publish": "npm run build && npm run npm:publish",
    "npm:build:publish:beta": "npm run build && cd libs/ngrx-entity-crud/ && npm publish --tag beta"
  },
  "dependencies": {
    "@angular/animations": "~13.2.6",
    "@angular/cdk": "~13.2.6",
    "@angular/common": "~13.2.6",
    "@angular/compiler": "~13.2.6",
    "@angular/core": "~13.2.6",
    "@angular/forms": "~13.2.6",
    "@angular/platform-browser": "~13.2.6",
    "@angular/platform-browser-dynamic": "~13.2.6",
    "@angular/router": "~13.2.6",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/entity": "^13.0.2",
    "@ngrx/router-store": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "rxjs": "~7.5.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.6",
    "@angular-devkit/schematics": "^13.2.6",
    "@angular-devkit/schematics-cli": "^13.2.6",
    "@angular-devkit/core": "^13.2.6",
    "@angular/cli": "~13.2.6",
    "@angular/compiler-cli": "~13.2.6",
    "@angular/language-service": "^13.2.6",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^17.0.21",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "codelyzer": "^6.0.0",
    "ng-packagr": "^13.2.1",
    "typescript": "~4.4.2"
  }
}
